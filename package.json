    "jss-props-sort": "^1.0.0",
    "jss-px": "^1.0.0",
    "jss-vendor-prefixer": "^1.0.0",
    "pixi-svg": "^1.0.1",
    "pixi.js": "^4.5.3",
    "two.js": "jonobr1/two.js#dev"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
